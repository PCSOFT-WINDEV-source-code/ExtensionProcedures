      -
        code : |1+
         
         sText	is string
         sResult	is string
         
         // Initial text
         sText	= "This is    a plain  TEXT sample    with WORDS    in      UPPERCASE and  series of   spaces."
         // Applies several transformations to this text:
         // - remove double spaces,
         // - changes everything to lowercase
         // - capitalizes the first letter of each word
         sResult	= sText.RemoveDoubleSpace().Lower().CapitalizeEachWord()
         
         Info("Initial text:", sText, "", "Result text:", sResult)
         
         
         
         arrIntegers is array of int
         
         // Initial array
         arrIntegers = [1, 4, 6, 8, 2, 9, 3, 7, 2, 3, 6, 4, 7]
         // Gets the sum of duplicated and triplicated elements
         nResult is int = arrIntegers.Multiply(3).Distinct().Sum()
         
         Info("Initial array: ", ArrayToString(arrIntegers, " , "), "", "Total duplicated and triplicated elements:", nResult)
         
        type : 18
      -
